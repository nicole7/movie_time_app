<div class="container">
  <h1><%= current_user.username %>'s Profile Page</h1>
  <% if current_user.orders.empty?  %>
    <%= "You do not have movie ticket purchases yet."%>
    <% else %>
    <%= link_to 'Purchase', '/attendings/new' %>
    <%=" a attending or "%>
    <% end %>
    <% current_user.orders.each do |attending| %>
      <%= attending %>
    <% end %>
</div>